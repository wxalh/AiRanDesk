{
  "name": "airandesk",
  "version": "3.0.0",
  "description": "AiRanDesk remote desktop application with advanced screen capture and H264 encoding",
  "dependencies": [
    "qt5-base",
    "qt5-websockets",
    "qt5-multimedia",
    "spdlog",
    "libdatachannel",
    {
      "name": "ffmpeg",
      "features": [
        "avcodec",
        "avformat",
        "avfilter",
        "swscale",
        "nvenc",
        "qsv",
        "amf"
      ]
    }
  ]
}
